# problem https://leetcode.com/explore/challenge/card/july-leetcoding-challenge/544/week-1-july-1st-july-7th/3381/

# solution 

class Solution:
    def hammingDistance(self, x: int, y: int):
        x='{:032b}'.format(x)
        y='{:032b}'.format(y)
        count=0
        for i in range(32):
            if x[i]!=y[i]:
                count+=1
                
        return count
        
